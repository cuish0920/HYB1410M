{
  "name": "HYB1410M_9.0.0",
  "version": "1.0.0",
  "author": "cui_sh",
  "description": "cui_sh",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": ""
  },
  "build": {
    "productName": "HYB1410M",
    "appId": "com.HYB1410M.app",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "asar": {
      "smartUnpack": true
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/hhit.png"
    },
    "linux": {
      "icon": "build/icons"
    },
    "extraResources": [
      "./extraResources/**",
      {
        "from": "./extraResources/template.hdr",
        "to": "../template.hdr"
      },
      {
        "from": "./extraResources/opencv_world347.dll",
        "to": "../opencv_world347.dll"
      },
      {
        "from": "./extraResources/MathParser_MD_VC110_v2_4.dll",
        "to": "../MathParser_MD_VC110_v2_4.dll"
      },
      {
        "from": "./extraResources/Log4cpp_MD_VC110_v2_4.dll",
        "to": "../Log4cpp_MD_VC110_v2_4.dll"
      },
      {
        "from": "./extraResources/Log_MD_VC110_v2_4.dll",
        "to": "../Log_MD_VC110_v2_4.dll"
      },
      {
        "from": "./extraResources/IpxCTI.cti",
        "to": "../IpxCTI.cti"
      },
      {
        "from": "./extraResources/IpxCameraApi.dll",
        "to": "../IpxCameraApi.dll"
      },
      {
        "from": "./extraResources/GenApi_MD_VC110_v2_4.dll",
        "to": "../GenApi_MD_VC110_v2_4.dll"
      },
      {
        "from": "./extraResources/GCBase_MD_VC110_v2_4.dll",
        "to": "../GCBase_MD_VC110_v2_4.dll"
      },
      {
        "from": "./extraResources/log4cpp.dll",
        "to": "../log4cpp.dll"
      },
      {
        "from": "./extraResources/log4cpp.properties",
        "to": "../log4cpp.properties"
      },
      {
        "from": "./extraResources/cui_test.db",
        "to": "../cui_test.db"
      }
    ]
  },
  "dependencies": {
    "HY_9010_T": "file:../HY_9010_T",
    "axios": "^0.21.1",
    "echarts": "^4.7.0",
    "element-ui": "^2.13.2",
    "hy_bsd": "^1.0.1",
    "sqlite3": "^4.1.1",
    "vue": "^2.6.11",
    "vue-devtools": "^5.1.3",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.15.7",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "vuex-electron": "^1.0.3"
  },
  "devDependencies": {
    "ajv": "^6.12.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.8.1",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "del": "^5.1.0",
