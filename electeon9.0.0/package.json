    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.1",
    "karma-electron": "^6.3.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.1",
    "multispinner": "^0.2.1",
